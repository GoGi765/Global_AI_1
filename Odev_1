#Odd numbers list and even numbers list
db = list(range(20))
oddlist=list()
evenlist=[]

for i in db:
    if i % 2 != 0:
        oddlist.append(i)
    else:
        evenlist.append(i)

print(oddlist)
print(evenlist)

#Merge lists
newlist=oddlist+evenlist
print(evenlist)

#Multiply lists by 2
newlist=[i*2 for i in newlist]
print(newlist)

#Print all the types
for i in newlist:
    print(type(i))
